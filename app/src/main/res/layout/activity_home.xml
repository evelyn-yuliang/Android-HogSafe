<?xml version="1.0" encoding="utf-8"?>

<!-- Google Pixel 2 - 2 -->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/google_pixe"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/mainpage"


    android:backgroundTintMode="screen"
    android:clickable="false"
    tools:visibility="visible">

    <View
        android:id="@+id/rectangle_2"
        android:layout_width="270dp"
        android:layout_height="96dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="70dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="96dp"
        android:background="@color/textBlack"

        />

    <TextView
        android:id="@+id/hogsafe"
        android:layout_width="220dp"
        android:layout_height="66dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="95dp"
        android:layout_marginTop="111dp"
        android:fontFamily="sans-serif-condensed"
        android:gravity="center_horizontal|top"
        android:text="@string/hogsafe"

        android:textAppearance="@style/hogsafe" />

    <View
        android:id="@+id/rectangle_3"
        android:layout_width="111dp"
        android:layout_height="167dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="36dp"
        android:layout_marginTop="261dp"
        android:alpha="0.8"
        android:background="@color/black"
        android:textAlignment="gravity"
        android:textDirection="firstStrongRtl" />

    <TextView
        android:id="@+id/eat_safe_st"
        android:layout_width="100dp"
        android:layout_height="144dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="42dp"
        android:layout_marginTop="273dp"
        android:backgroundTint="#95888484"
        android:fontFamily="casual"
        android:gravity="center_horizontal|top"
        android:text="@string/eat_safe_st"
        android:textAppearance="@style/eat_safe_st"
        android:textStyle="bold" />

</RelativeLayout>
    <!-- drawable/google_pixe.xml -->

